    def belongs_to_categories

      doc.css(".globalCurrentCrumb a").select{|elem| elem["href"] && elem["href"] != "http://www.binggo.com/" && elem["href"].to_s !~ /brand|html/}.map do |elem|
        {
          :name => elem.inner_text,
          :url  => elem["href"]
        }
      end

    end

		def title
			doc.css(".productName h1").text
		end

		def price
			doc.css("div#price").text
			#return str.to_f
		end

		def stock
			return 1
		end

